library(shiny)
# Define UI
ui <- fluidPage(
tags$head(
tags$style(HTML("
.green-button {
background-color: green;
color: white;
}
.grey-button {
background-color: grey;
color: white;
}
"))
),
titlePanel("Change Button Color"),
mainPanel(
actionButton("button", icon("check"), class = "green-button")
)
)
# Define server
server <- function(input, output) {
# Change button color when clicked
observeEvent(input$button, {
if (input$button %% 2 == 0) {
updateActionButton(session, "button", icon("check"), class = "green-button")
} else {
updateActionButton(session, "button", icon("check"), class = "grey-button")
}
})
}
# Run app
shinyApp(ui = ui, server = server)
